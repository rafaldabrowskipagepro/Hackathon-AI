{
  "name": "hackathon-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "start": "turbo start",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "dev:site": "turbo dev --filter=site",
    "dev:cms": "turbo dev --filter=cms",
    "build:site": "turbo build --filter=site",
    "build:cms": "turbo build --filter=cms"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "turbo": "^2.5.3"
  },
  "resolutions": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  }
}
